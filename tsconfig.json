{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "moduleResolution": "bundler",
    "allowJs": true,
    "noEmit": true,
    "isolatedModules": true
  },
  "include": [
    "app/**/*.ts",
    "app/**/*.tsx",
    "expo-env.d.ts",
    ".expo/types/**/*.ts"
  ]
}